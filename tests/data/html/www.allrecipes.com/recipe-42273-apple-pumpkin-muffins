                <script type="text/javascript">
                // Find the EU DSAR link by link text.
                function getEUDSARlink() {
                    let elem;
                    const links = document.getElementsByTagName('a');
                    for (let link of links) {
                        if (link.innerHTML == 'EU Data Subject Requests') {
                        elem = link;
                        break;
                        }
                    }
                return elem;
                };
                // Pass EU DSAR link element to the link-hiding function.
                (function (elem) {
                    if (elem && typeof elem !== 'undefined') {
                        const d = window.document;
                        const now = Math.floor(Date.now() / 1000);
                        const newYear2020 = 1577836800;
                        if (now >= newYear2020 || d.location.hash == '#ccpa') {
                        elem.style.display = 'none';
                        }
                    }
                }(getEUDSARlink()));
                </script>
