<script type="text/javascript">
    // Set window.refHub if this is the first recipe visited after visiting a hub.
    // TODO: Do we care if user refreshes the page?
    var hubId = window.sessionStorage["Ref_Hub_Id"];
    var count = window.sessionStorage["Ref_Hub_Recipe_Count"];

    if (hubId && count) {
        var newCount = parseInt(count) + 1;
        window.sessionStorage.setItem("Ref_Hub_Recipe_Count", newCount);
        if (newCount <= 1) {
            window.refHub = hubId;
        }
    }
</script>


