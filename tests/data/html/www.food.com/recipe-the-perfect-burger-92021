            {{? it.replies && it.replyTotal > 0}}
            <a class="fd-af-replies-toggle" href="#">
                <span class="af-like-label">Comments</span>
                <span class="af-reply-count">{{= it.replyTotal}}</span>
            </a>
            {{?? it.type !== 'created'}}
            <a class="fd-af-leave-reply" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}" href="#">Comment</a>
            {{?}}
        </div>
        {{?}}

