<div id="ad-is-tablet"></div>
<script>
    (function($) {
        
        window.adConfiguration = {
            "settings": {
                "responsiveGridSlots": 0 }
        };

        
        var mobileAdElem = document.getElementById('ad-is-mobile');
        var isMobileAds = !(mobileAdElem.offsetWidth === 0 && mobileAdElem.offsetHeight === 0);


        var isTablet = $ && $('#ad-is-tablet').css('display') === 'none';

         
        window.adService = {
            isDesktop: window.innerWidth > 1024,
            mobileAds: isMobileAds,
            tabletAds: isTablet,
            unitValues: {
                channel: "appetizer"
            },
            pageTargetingValues: {
                type: "recipe",
                mention_category: "",
                mention: "",
                "status": "unrecognized",
                "oid": "",
                "fit": "1",
                "r": "139917",
                "id": "139917",
                "k": [3,10,49,58,85,106,125,145,173,184,201,208,214,221,230,235,241,257,261,607,682,703]
            },
            suppressInterstitial: true
        };

    })($ || window.jQuery);
</script>

