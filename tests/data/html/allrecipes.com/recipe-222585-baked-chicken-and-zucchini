            </div>
            <ul class="info">
                <li><%= this.name %></li>
                <li><%= this.address %></li>
                <li><%= this.city %>, <%= this.state %> <%= this.zip %></li>
                <li class="advertisement"><%= this.sponsoredFlag == true ? "Sponsored" : "" %> </li>
            </ul>
        </div>
    ]]>
</script>



