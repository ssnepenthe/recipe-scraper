       function ti_amg_fwrd_custom_ajax_node_based_outbrain() {
         var content_id = utag_data.content_id;
         link = "/custom-outbrain/" + content_id;
         jQuery("#outbrain-wrapper").load(link);
       }
       function ti_amg_fwrd_custom_bottom_outbrain_ajax_load() {
         jQuery("#outbrain-wrapper").load("/outbrain-bottom");
