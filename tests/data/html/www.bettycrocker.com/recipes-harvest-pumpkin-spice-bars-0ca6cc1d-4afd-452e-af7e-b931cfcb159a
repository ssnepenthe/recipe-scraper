});

// Consent update event handler
var OneTrustCurrentGroupData = "";
var OneTrustDefaultGroupData = "C0001-C0003-C0002-C0004";
function OneTrustUpdateEventHandler(ev) {
    // Event fires initially when banner loads
    // If first view of banner, groups is empty
    // If preference update, groups will have at least one (strictly necessary)
    var groupData = ev.detail == "" ? OneTrustDefaultGroupData : ev.detail.join('-')

    if (OneTrustCurrentGroupData == "") {
        OneTrustCurrentGroupData = groupData;
    }
    else {
        // if preferences have changed, reload page
        if (OneTrustCurrentGroupData != ev.detail.join('-')) {
            // If functional is not present, remove DNM geolocation cookie
            if (ev.detail.indexOf("C0003") < 0) {
                document.cookie = 'DNMselectedZipCode=;path=/;expires=Thu, 01 Jan 1970 00:00:00 GMT';
                document.cookie = 'cellfire.location=;path=/;expires=Thu, 01 Jan 1970 00:00:00 GMT';
            }
            window.location.reload(true);
        }
    }
    // console.log("Window event captured");
    // console.log(ev);
}

