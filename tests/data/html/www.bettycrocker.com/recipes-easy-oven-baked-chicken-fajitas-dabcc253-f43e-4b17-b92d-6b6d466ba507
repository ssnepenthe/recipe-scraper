var OneTrustShowPreferenceCenter = function () {
    if (window.OneTrust && typeof window.OneTrust.ToggleInfoDisplay === "function") {
        window.OneTrust.ToggleInfoDisplay();
    }
    else {
        // One Trust script not yet loaded
        // Let's only run this for around 5 seconds, in case we can't load the script
        OneTrustLoadScript();
        var checkCount = 0;
        var otCheck = setInterval(function () {
            checkCount++;
            if (window.OneTrust && typeof window.OneTrust.ToggleInfoDisplay === "function") {
                window.OneTrust.ToggleInfoDisplay();
                clearInterval(otCheck);
            }
            if (checkCount > 20) {
                clearInterval(otCheck);
            }
        }, 250); 
    }
}

var cookieExists = window.GeneralMills.getGlobalSetting("userPrivacy_CookieExists");
// Check if we need to present the banner
// If so, show immediately for non-recipe, scroll-deferred for RDP
if (!cookieExists) {
    var osPageType = document.querySelector("meta[property='onespot:page-type']");
    var isRecipePage = osPageType && osPageType.getAttribute('content') == "Recipe";
    if (isRecipePage) {
        var componentMetadata = {};
        componentMetadata.viewName = "OneTrust";
        componentMetadata.componentName = "OneTrust_ScriptInclude";
        componentMetadata.deferOptions = { deferComponent: true, deferType: "Simple" };
        GeneralMills.ComponentRegistrar.registerComponent(function () {
            OneTrustLoadScript();
        }, componentMetadata);
    }
    else {
        OneTrustLoadScript();
    }
}

// Capture clicks from ShowPreferenceCenter links
