                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients">1 cup shredded extra-sharp Cheddar cheese</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients"> cup finely diced smoked ham</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients"> cup reduced-fat sour cream</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients"> cup minced fresh chives</span>
                    </li>
                    <li class="checkListListItem checkListLine ">
                        <span itemprop="ingredients"> teaspoon freshly ground pepper</span>
                    </li>

        </ul>

    
