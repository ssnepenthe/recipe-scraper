<script type="text/javascript">
    /*
    * This code handles absolutely positioned element who's width and height that could not be done using css
    * It sits here to be executed as soon as DOM for that element is ready to prevent any unwanted visual effects
    */

    FD = FD || {};
    FD.TopTileOnResizeHandler = (function () {
        var element,
            elementContainer,
            topTileImageContainer,
            smWrapper = 294,
            mdWrapper = 749,
            lgWrapper = 994,
            xlWrapper = 1239,
            elementWidthConfig,
            gridGutterWidth = 14,
            gridColumnWidth = 21,
            containerGridSise,
            currViewportName,
            containerWidth;

        return {
            init: function () {
                element = document.getElementsByClassName('info-wrapper')[0];
                elementContainer = document.getElementsByClassName('js-full-grid-container')[0];
                topTileImageContainer = document.getElementsByClassName('img-wrapper')[0];
                containerGridSise = {
                    xs: {cols: 8, gutters: 7},
                    sm: {cols: 8, gutters: 7},
                    md: {cols: 8, gutters: 7},
                    lg: {cols: 10, gutters: 9}
                };
                elementWidthConfig = {
                    xsSelfWidth: (containerGridSise.xs.cols * gridColumnWidth + containerGridSise.xs.gutters * gridGutterWidth),
                    xs: (containerGridSise.xs.cols * gridColumnWidth + containerGridSise.xs.gutters * gridGutterWidth) / smWrapper,
                    sm: (containerGridSise.sm.cols * gridColumnWidth + containerGridSise.sm.gutters * gridGutterWidth) / mdWrapper,
                    md: (containerGridSise.md.cols * gridColumnWidth + containerGridSise.md.gutters * gridGutterWidth) / lgWrapper,
                    lg: (containerGridSise.lg.cols * gridColumnWidth + containerGridSise.lg.gutters * gridGutterWidth) / xlWrapper
                };

            },
            adjustSize: function () {
                currViewportName = FD.viewports.viewport().name;
                containerWidth = elementContainer.offsetWidth * elementWidthConfig[currViewportName];
                if (FD.viewports.viewport().name == 'xs') element.style.width = containerWidth + 'px';
                var elementHeight =
                    document.getElementsByClassName('top-tile')[0].className.indexOf('no-photo') !== -1
                    ? (FD.viewports.viewport().name =='xs' ? document.getElementsByClassName('make-it-shine')[1].offsetHeight : document.getElementsByClassName('make-it-shine')[0].offsetHeight)
                    : (FD.viewports.viewport().name =='xs' ? topTileImageContainer.offsetHeight - 20 : topTileImageContainer.offsetHeight);

                element.style['max-height'] = elementHeight + 'px';
                //fix for IE11
                if (FD.viewports.viewport().name != 'xs') {
                    var feedInfoWrapperMaxHeight = elementHeight - document.getElementsByClassName('chef-note')[0].scrollHeight;
                    document.getElementsByClassName('feed-info-wrapper')[0].style['max-height'] = feedInfoWrapperMaxHeight + 'px';
                }
            }
        }
    })();
    FD.TopTileOnResizeHandler.init();
    FD.TopTileOnResizeHandler.adjustSize();
</script>

