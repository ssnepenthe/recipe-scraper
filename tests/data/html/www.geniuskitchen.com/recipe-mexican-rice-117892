                <div class="user-info">
                    <div> <a href="{{=it.profileUrl}}">@{{=it.memberName}}</a> </div>
                    <div class="status">{{? it.userStatus}}{{=it.userStatus}}{{?}}</div>
                </div>

                <div class="fd-list-table">
                    <ul>
                        {{? it.recipesCount > 0}}
