<script type="text/html-template" id="fd-af-dialog">
   {{? !((it.type === 'all' || it.type === 'reviews') && it.hasPinnedItems) }}
    <h4>{{= it.title}}</h4>
   {{?}}
    <div class="fd-af-input-container {{= it.type}}{{? (it.type === 'all' || it.type === 'reviews') && it.hasPinnedItems }} hidden{{?}}">
        <div class="fd-af-input-header">
            <div class="avatar-wrapper">
                
            </div>
            <div>
                {{? it.type === 'all' || it.type === 'reviews'}}
                <div class="js-af-dialog-rating fd-rate-system">
                    <span class="stars-container">
                        <span class="fa star r5" data-rating="5"></span>
                        <span class="fa star r4" data-rating="4"></span>
                        <span class="fa star r3" data-rating="3"></span>
                        <span class="fa star r2" data-rating="2"></span>
                        <span class="fa star r1" data-rating="1"></span>
                    </span>
                </div>
                {{?}}
            </div>
        </div>
        <form id="fd-af-form" data-type="{{= it.type}}">
            <div class="fd-af-user-input-container">
                <textarea class="fd-af-user-input" form="fd-af-form" placeholder="{{= it.placeholderText}}" name="text"></textarea>
