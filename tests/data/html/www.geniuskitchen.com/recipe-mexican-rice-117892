            <a class="af-like{{? it.counts.liked}} liked{{?}}" data-item-id="{{= it.id}}" data-item-type="{{= it.type}}" href="#">
                <i class="icon-fdc-like"></i>
                <span class="af-like-label">Like</span>
                <span class="af-like-count">{{? it.counts.like > 0}}{{= it.counts.like}}{{?}}</span>
            </a>
        </div>
        {{?}}

